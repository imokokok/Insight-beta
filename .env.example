# Database (recommended)
DATABASE_URL=

# Optional: Supabase Postgres URL for provisioning/fallback
SUPABASE_DB_URL=

# Optional: used by scripts/legacy fallback
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Admin APIs auth (use header x-admin-token or Authorization: Bearer)
INSIGHT_ADMIN_TOKEN=
INSIGHT_ADMIN_TOKEN_SALT=

# Default oracle settings (used as defaults; can also be set via /api/oracle/config)
INSIGHT_RPC_URL=
INSIGHT_ORACLE_ADDRESS=
INSIGHT_CHAIN=Local

# Proxy / networking
# INSIGHT_TRUST_PROXY=false
# INSIGHT_TRUST_PROXY=cloudflare

# Rate limiting storage
# INSIGHT_RATE_LIMIT_STORE=auto

# API logging (sample rate 0..1)
# INSIGHT_API_LOG_SAMPLE_RATE=0.01

# Memory mode tuning (only used when DATABASE_URL is not set)
# INSIGHT_MEMORY_MAX_VOTE_KEYS=200000
# INSIGHT_MEMORY_VOTE_BLOCK_WINDOW=50000

# Client-side (safe to expose)
NEXT_PUBLIC_INSIGHT_ORACLE_ADDRESS=
