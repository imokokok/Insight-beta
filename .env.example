# Database (Required)
DATABASE_URL=

# Optional: Supabase Postgres URL for provisioning/fallback
SUPABASE_DB_URL=
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Admin APIs auth (Required for secure access)
# Use 'x-admin-token' header or 'Authorization: Bearer <token>'
INSIGHT_ADMIN_TOKEN=
INSIGHT_ADMIN_TOKEN_SALT=

# Oracle / Chain Configuration
# INSIGHT_RPC_URL can be a comma-separated list of URLs
INSIGHT_RPC_URL=
INSIGHT_ORACLE_ADDRESS=
INSIGHT_CHAIN=Local

# Worker & Performance Tuning
# Set to 'true' to run worker separately (e.g. in a background job)
# INSIGHT_DISABLE_EMBEDDED_WORKER=false
# INSIGHT_WORKER_ID=embedded
# INSIGHT_MEMORY_MAX_ASSERTIONS=10000
# INSIGHT_MEMORY_MAX_DISPUTES=10000
# INSIGHT_MEMORY_MAX_VOTE_KEYS=200000
# INSIGHT_MEMORY_VOTE_BLOCK_WINDOW=50000

# Networking & Security
# INSIGHT_TRUST_PROXY=false
# INSIGHT_TRUST_PROXY=cloudflare
# INSIGHT_RATE_LIMIT_STORE=auto
# INSIGHT_API_LOG_SAMPLE_RATE=0.01

# Timeouts (ms)
# INSIGHT_DEPENDENCY_TIMEOUT_MS=
# INSIGHT_RPC_TIMEOUT_MS=
# INSIGHT_WEBHOOK_TIMEOUT_MS=

# Notifications
# INSIGHT_WEBHOOK_URL=
# INSIGHT_SMTP_HOST=
# INSIGHT_SMTP_PORT=
# INSIGHT_SMTP_USER=
# INSIGHT_SMTP_PASS=
# INSIGHT_FROM_EMAIL=
# INSIGHT_DEFAULT_EMAIL=

# Client-side (safe to expose)
NEXT_PUBLIC_INSIGHT_ORACLE_ADDRESS=
